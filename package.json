    "typescript",
    "rest",
    "api-generator",
    "database",
    "prisma",
    "typebox",
    "fastify",
    "typedi",
    "dependency-injection"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unbywyd/tsdiapi-crud"
  },
